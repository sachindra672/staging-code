  unbannedAt        DateTime?
  bannedByAdmin     admin?    @relation("BannedByAdmin", fields: [bannedByAdminId], references: [id])
  bannedByMentor    mentor?   @relation("BannedByMentor", fields: [bannedByMentorId], references: [id])
  student           endUsers  @relation("UserBans", fields: [studentId], references: [id])
  unbannedByAdmin   admin?    @relation("UnbannedByAdmin", fields: [unbannedByAdminId], references: [id])
}

model CourseMaterial {
  id          Int            @id @default(autoincrement())
  fileName    String
  fileUrl     String
  bigCourseId Int
  sessionId   Int?
  storageType String         @default("GCS")
  source      MaterialSource @default(NEW)
  createdOn   DateTime       @default(now())
  bigCourse   BigCourse      @relation(fields: [bigCourseId], references: [id])
  session     session?       @relation(fields: [sessionId], references: [id])

  @@index([bigCourseId])
  @@index([sessionId])
}

