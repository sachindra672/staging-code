  imageUrl     String?
  thumbnailUrl String?

  // navigation
  actionType   String? // OPEN_PAGE, OPEN_URL, NONE
  actionTarget String?
  actionData   Json?

  source    AnnouncmentSource @default(ADMIN)
  createdBy Int?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  isDeleted Boolean  @default(false)

  user   endUsers?  @relation(fields: [userId], references: [id])
  course BigCourse? @relation(fields: [courseId], references: [id])
  mentor mentor?    @relation(fields: [mentorId], references: [id])

  reads AnnouncementRead[]

  @@index([status])
  @@index([scheduledAt])
  @@index([audience, scope])
}

model AnnouncementRead {
  id String @id @default(cuid())

  announcementId String
  userId         Int?
  mentorId       Int?

  readAt DateTime @default(now())

  announcement Announcement @relation(fields: [announcementId], references: [id], onDelete: Cascade)

  user   endUsers? @relation(fields: [userId], references: [id], onDelete: Cascade)
  mentor mentor?   @relation(fields: [mentorId], references: [id], onDelete: Cascade)

  @@unique([announcementId, userId])
  @@unique([announcementId, mentorId])
  @@index([userId])
  @@index([mentorId])
}

//ban system
model BanRecord {
  id                Int       @id @default(autoincrement())
  studentId         Int
  bannedByMentorId  Int?
  bannedByAdminId   Int?
  unbannedByAdminId Int?
  reason            String
  bannedAt          DateTime  @default(now())
  unbannedAt        DateTime?
  student           endUsers  @relation("UserBans", fields: [studentId], references: [id])
  bannedByMentor    mentor?   @relation("BannedByMentor", fields: [bannedByMentorId], references: [id])
  bannedByAdmin     admin?    @relation("BannedByAdmin", fields: [bannedByAdminId], references: [id])
  unbannedByAdmin   admin?    @relation("UnbannedByAdmin", fields: [unbannedByAdminId], references: [id])
}
